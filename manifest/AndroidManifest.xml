<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SatSet"
        tools:targetApi="31">
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.SinopsisFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.ShareFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.SettingFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.AboutFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.HistoryFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.ProfilFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.ScheduleFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.MainFragment"
            android:exported="false" />
        <activity
            android:name="androidx.constraintlayout.widget.FragmentMovie.HomeFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:label="SatSet">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="androidx.constraintlayout.widget.Alarm.AlarmReceiver" />

        <activity
            android:name=".MenuActivity"
            android:exported="false" />
        <activity
            android:name=".MapActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <data android:scheme="geo" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HelloActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ScrollingIcecold"
            android:exported="false" />
        <activity
            android:name="androidx.constraintlayout.widget.Alarm.AlarmActivity"
            android:exported="false" />
        <activity
            android:name="androidx.constraintlayout.widget.Second.OneActivity"
            android:exported="false" />
        <activity
            android:name=".FibonacciActivity"
            android:exported="false" />
        <activity
            android:name="androidx.constraintlayout.widget.Second.SecondActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>